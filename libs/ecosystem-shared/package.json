{
  "name": "@trikztime/ecosystem-shared",
  "version": "1.0.12",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "copypackage": "cp -rf ./package.json ./dist/package.json",
    "build": "rm -rf dist/ && tsc && npm run copypackage",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish-build": "rm -rf dist/ && tsc",
    "publish-package": "npm run publish-build && npm run copypackage && cd dist && npm publish"
  },
  "author": "not-log",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/microservices": "^9.3.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
