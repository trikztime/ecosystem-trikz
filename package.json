{
  "name": "ecosystem-trikz",
  "version": "1.0.0",
  "scripts": {
    "build-lib": "cd @dev-scripts && sh ./build-lib.sh",
    "link-lib": "cd @dev-scripts && sh ./link-lib.sh",
    "dev": "npm-run-all --print-label --parallel dev-gateway dev-gateway-socket dev-api dev-discord dev-skillrank dev-steam",
    "dev-gateway": "cd services/gateway && npm run start:dev",
    "dev-gateway-socket": "cd services/gateway-socket && npm run start:dev",
    "dev-api": "cd services/api && npm run start:dev",
    "dev-discord": "cd services/discord && npm run start:dev",
    "dev-skillrank": "cd services/skillrank && npm run start:dev",
    "dev-steam": "cd services/steam && npm run start:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trikztime/ecosystem-trikz.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
